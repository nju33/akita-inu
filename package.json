    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "7.1.0",
    "pupa": "^1.0.0"
  },
  "devDependencies": {
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-config-xo": "^0.24.2",
    "jest": "23.5.0",
    "nodemon": "1.18.3",
    "smee-client": "1.0.2",
    "standard": "12.0.0"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "./src"
    ]
  }
}
