{
  "name": "akita-inu",
  "version": "0.0.0",
  "description": "A Probot app",
  "author": "nju33 <nju33.ki@gmail.com>",
  "license": "MIT",
  "repository": "nju33:akita-inu",
  "homepage": "https://github.com/nju33/akita-inu",
  "bugs": "https://github.com/nju33/akita-inu/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
